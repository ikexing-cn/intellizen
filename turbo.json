{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^langium:generate", "^build"],
      "outputs": ["out/**"],
      "cache": false
    },
    "dev": {
      "dependsOn": ["^langium:generate"],
      "cache": false
    },
    "langium:generate": {
      "cache": false
    }
  }
}
